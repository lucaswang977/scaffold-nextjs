{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["testing-library", "prettier"],
  "rules": {
    "prettier/prettier": ["warn"]
  },
  "parser": "@typescript-eslint/parser",
  "overrides":[
    {
      "files": [
        "**/__tests__/**/*.[jt]s?(x)",
        "**/?(*.)+(spec|test).[jt]s?(x)"
      ],
      "extends": ["plugin:testing-library/react"]
    }
  ] 
}
